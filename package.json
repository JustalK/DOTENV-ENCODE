{
  "name": "dotenv-encode",
  "version": "1.0.0",
  "description": "",
  "main": "dist/es5/src/cli.js",
  "preferGlobal": true,
  "bin": {
    "secure-env": "dist/es5/src/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "dist/es5/src/cli.js",
    "run:encode": "node ./dist/cli.js ./env/prod.env -o ./env_encrypted/prod.env -s mySqsdqsdecretKey",
    "run:decode": "node ./dist/cli.js ./env_encrypted/prod.env -o ./env/prod.env -s mySqsdqsdecretKey -d",
    "lint": "npx eslint --fix src/*.ts",
    "prebuild": "npm run build:delete && npm run lint",
    "build": "npx tsc",
    "build:delete": "rimraf dist/es5",
    "prepare": "ts-patch install -s"
  },
